generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL_PRISMA")
  relationMode = "prisma"
}

model games {
  id       String   @default(cuid()) @id
  name     String? @db.VarChar(255)
  platform String? @db.VarChar(255)
  series   String? @db.VarChar(255)
}